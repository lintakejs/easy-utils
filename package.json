    "@typescript-eslint/typescript-estree": "^2.28.0",
    "axios": "^0.20.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.23.4",
