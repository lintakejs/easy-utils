    "build:package": "npm-run-all build:clean compile && node ./tools/generate-alias.js",
    "unit": "jest --config test/unit/jest.conf.js",
    "lint": "eslint --ext .js,.ts src --fix",
    "test": "npm run unit"
  },
  "peerDependencies": {
    "axios": ">=0.19.2",
    "rxjs": ">=6.0.0"
  },
  "devDependencies": {
