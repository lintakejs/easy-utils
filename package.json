    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "fs-extra": "^9.0.1",
    "jest": "^25.4.0",
    "jest-serializer-vue": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rxjs": "^6.6.6",
    "shx": "^0.3.2",
    "ts-jest": "^26.4.2",
    "typescript": "^3.8.3",
